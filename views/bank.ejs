      <div class='col-sm-6' id="tärningar" style="text-align: right;">
        <h1 style="display:inline-block; margin: 15px 5px 15px 5px;">{{d}}</h1>
        <div class="dice" style="-webkit-transform: rotate({{7*d-25}}deg);">
          <div style="left: 8px; top: 8px;" ng-show="d==3 || d==4 || d==6 || d==7 || d==9 || d==10 || d==11 || d==12"></div>
          <div style="left: 32px; top: 8px;" ng-show="d==5 || d==6 || d==8 || d==9 || d==10 || d==11 || d==12"></div>
          <div style="left: 8px; top: 32px;" ng-show="d==5 || d==6 || d==8 || d==9 || d==10 || d==11 || d==12"></div>
          <div style="left: 32px; top: 32px;" ng-show="d==3 || d==4 || d==6 || d==7 || d==9 || d==10 || d==11 || d==12"></div>
          <div style="left: 20px; top: 20px;" ng-show="d==2 || d==7 || d==10"></div>
          <div style="left: 8px; top: 20px;" ng-show="d==9 || d==11 || d==12"></div>
          <div style="left: 32px; top: 20px;" ng-show="d==9 || d==11 || d==12"></div>
        </div>
        <div class="dice" style="-webkit-transform: rotate(-{{d*7+5}}deg);">
          <div style="left: 8px; top: 8px;" ng-show="d==5 || d==6 || d==7|| d==8 || d==10 || d==11 || d==12"></div>
          <div style="left: 32px; top: 8px;" ng-show="d==4 || d==7|| d==8 || d==9 || d==10 || d==11 || d==12"></div>
          <div style="left: 8px; top: 32px;" ng-show="d==4 || d==7|| d==8 || d==9 || d==10 || d==11 || d==12"></div>
          <div style="left: 32px; top: 32px;" ng-show="d==5 || d==6 || d==7|| d==8 || d==10 || d==11 || d==12"></div>
          <div style="left: 20px; top: 20px;" ng-show="d==2 || d==3 || d==5 || d==9 || d==10 || d==11"></div>
          <div style="left: 8px; top: 20px;" ng-show="d==8 || d==12"></div>
          <div style="left: 32px; top: 20px;" ng-show="d==8 || d==12"></div>
        </div>
      </div>
    </div>
    <div id="vänsterspalt" class="col-sm-6">
      <svg height="300" width="520" class="stockmarket">
